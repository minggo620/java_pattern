package pattern.composite;

import java.util.ArrayList;
import java.util.List;

public abstract class AbstractCompositor extends AbstractEmployee {
	
	protected List<AbstractEmployee> employeeList;
	
	public AbstractCompositor(){
		this.employeeList = new ArrayList<>();
	}
	
	public abstract void addEmployee(AbstractEmployee employee);
	public abstract void removeEmployee(String name);
	public abstract void infoEmployee();
	
	
}
